# Supabase Configuration
# These environment variables are REQUIRED for production builds
# Copy this file to .env and fill in the actual values

# Supabase Project URL
# Format: https://<project-id>.supabase.co
# Get from: Supabase Dashboard → Settings → API
VITE_SUPABASE_URL=https://your-project.supabase.co

# Supabase Anon (Public) Key
# Format: eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
# Get from: Supabase Dashboard → Settings → API → anon key
VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Application Configuration
# Environment name for logging and monitoring
VITE_APP_ENV=production

# Debug logging (false in production)
VITE_DEBUG=false

# Production Safety
# These should always be enabled in production builds
VITE_USE_SUPABASE=true
VITE_ENFORCE_RLS=true
VITE_REQUIRE_AUTH=true
